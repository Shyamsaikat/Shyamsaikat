{

    “builds”: [{
    
    “src”: “pstu_transport_section/wsgi.py”,
    
    “use”: “@vercel/python”,
    
    “config”: { “maxLambdaSize”: “15mb”, “runtime”: “python3.9” }
    
    }],
    
    “routes”: [
    
    {
    
    “src”: “/(.*)”,
    
    “dest”: “pstu_transport_section/wsgi.py”
    
    }
    
    ]
    
    }